<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#FFFFFF">
	<TextView android:id="@+id/listText" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="@string/list_text"
		android:textColor="#000000" android:textSize="18px"></TextView>
	<ListView android:id="@+id/listView" android:layout_width="fill_parent"
		android:layout_height="365px"></ListView>
	<Button android:id="@+id/createButton" android:layout_width="100px"
		android:layout_height="40px" android:text="@string/create_button"
		android:layout_gravity="center_horizontal" android:textSize="18px"></Button>
</LinearLayout>
